package Chinmay;
import java.util.Scanner;
public class Fibonacci {
    private static Scanner in;

	public static void main(String args[]) {
    	  int a=0,b=1,c,n,i;
    	  in = new Scanner(System.in);
    	  System.out.println("Enter the range: ");
    	  n=in.nextInt();
    	  System.out.print("" +a+ " , " + b);
    	  for(i=0;i<n;i++) {
       		  c=a+b;
    		  System.out.print(", " +c);
    		  a=b;
    		  b=c;    		  
    	  }
    	  
      }
}
