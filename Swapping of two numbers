package Chinmay;

import java.util.Scanner;

public class swap {
     private static Scanner in;

	public static void main(String args[]) {
    	 int int1,int2;
    	 System.out.println("Enter the two values:");
    	 in = new Scanner(System.in);
    	 int1=in.nextInt();
    	 int2=in.nextInt();
    	 System.out.println("The original elements are int1=" + int1 + " int2=" + int2);
    	 int1=int1+int2;
    	 int2=int1-int2;
         int1=int1-int2;
         System.out.println("The swapped elements are int1=" + int1 + "int2=" + int2);
     }
}
