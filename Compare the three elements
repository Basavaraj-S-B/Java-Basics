package Chinmay;
import java.util.Scanner;
public class compare3 {
 public static void main(String args[]) {	
 int a,b,c;
 try (Scanner in = new Scanner(System.in)) {
	System.out.println("Enter the value of a: ");
	 a=in.nextInt();
	 System.out.println("Enter the value of b: ");
	 b=in.nextInt();
	 System.out.println("Enter the value of c: ");
	 c=in.nextInt();
}
 if(a>b && a>c) {
	 System.out.println("a is greater than the other two");
 }
 if(b>c && b>a) {
	 System.out.println("b is greater than the other two");
 }
 if(c>b && c>a) {
	 System.out.println("c is greater than the other two");
 }
 }

}
