package Chinmay;
import java.util.*;
public class ArrayListdemo {
   public static void main(String[] args) {
	   ArrayList<String> a1 = new ArrayList<String>();
	   System.out.println("Initial size of arraylist " + a1.size());
	   a1.add("F");
	   a1.add("A");
	   a1.add("K");
	   a1.add("T");
	   a1.add(2,"G3");
	   System.out.println("Final size of arraylist " + a1.size());
	   a1.remove(3);
	   System.out.println("Contents of array list: " + a1);
   }
}
