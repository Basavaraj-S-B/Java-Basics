package Chinmay;

import java.util.Scanner;

public class starPattern2 {
	public static void main(String args[]) {
		int i,j,n;
		Scanner in = new Scanner(System.in);
		System.out.println("Enter the range:");
		n=in.nextInt();
		for(i=n;i>=1;i--) {
			for(j=i;j>=1;j--) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
