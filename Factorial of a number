package Chinmay;
import java.util.*;
public class factorial {
    private static Scanner in;

	public static void main(String args[]) {
       int fact=1,i,n;
       System.out.println("Enter the number for which factorial has to be calculated");
       in = new Scanner(System.in);
       n=in.nextInt();
       for(i=1;i<=n;i++) {
    	   fact=fact*(i);
    	   
    	  
       }
       System.out.println("The factorial of " + n + " is " + fact );
    }
}
